@model List<_01_LampshadeQuery.Contracts.Account.UserMusicQueryModel>
	<section class="row row--grid">
				<!-- title -->
				<div class="col-12">
					<div class="main__title">
						<h2>گلچین دوستان</h2>

						<a asp-page="/UserMusic" class="main__link">دیدن همه <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17.92,11.62a1,1,0,0,0-.21-.33l-5-5a1,1,0,0,0-1.42,1.42L14.59,11H7a1,1,0,0,0,0,2h7.59l-3.3,3.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5a1,1,0,0,0,.21-.33A1,1,0,0,0,17.92,11.62Z"/></svg></a>
					</div>
				</div>
				<!-- end title -->

				<div class="col-12">
					<div class="main__carousel-wrap">
						<div class="main__carousel main__carousel--store owl-carousel" id="store">
							
							@foreach(var music in Model)
							{
							  <div class="product">
								 
								<a data-link data-title="@music.Name" data-artist="@music.Singer"  href="~/Cafee-Music/@music.Track" class="product__img">
								@if(music.Photo == "")
								{
									<img src="~/کافه.png"style="width:135px; height:135px" >
						        }
								else
							{
								<img src="~/Cafee-Music/@music.Photo"style="width:135px; height:135px" >
							}
							
								</a>
								<h3 class="product__title"><a href="product.html">@music.Name</a></h3>
								<span></span>
								<span class="product__price">@music.Singer</span>
								<span>
									<a href="~/Cafee-Music/@music.Track" class="single-item__export">
										<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M21,14a1,1,0,0,0-1,1v4a1,1,0,0,1-1,1H5a1,1,0,0,1-1-1V15a1,1,0,0,0-2,0v4a3,3,0,0,0,3,3H19a3,3,0,0,0,3-3V15A1,1,0,0,0,21,14Zm-9.71,1.71a1,1,0,0,0,.33.21.94.94,0,0,0,.76,0,1,1,0,0,0,.33-.21l4-4a1,1,0,0,0-1.42-1.42L13,12.59V3a1,1,0,0,0-2,0v9.59l-2.29-2.3a1,1,0,1,0-1.42,1.42Z"></path></svg>
								</a>
                                </span>
								<span class="product__new">@music.PublishDate</span>
								<span class="product__new">@music.UserName</span>
								
							   </div>
							}
							

							
						</div>
							<div>
						<p>

                        </p>
                    </div>
					<div>
						<p>

                        </p>
                    </div>
					<div>
						<button class="main__nav main__nav--prev" data-nav="#podcasts" type="button"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17,11H9.41l3.3-3.29a1,1,0,1,0-1.42-1.42l-5,5a1,1,0,0,0-.21.33,1,1,0,0,0,0,.76,1,1,0,0,0,.21.33l5,5a1,1,0,0,0,1.42,0,1,1,0,0,0,0-1.42L9.41,13H17a1,1,0,0,0,0-2Z"/></svg></button>
						<button class="main__nav main__nav--next" data-nav="#podcasts" type="button"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17.92,11.62a1,1,0,0,0-.21-.33l-5-5a1,1,0,0,0-1.42,1.42L14.59,11H7a1,1,0,0,0,0,2h7.59l-3.3,3.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5a1,1,0,0,0,.21-.33A1,1,0,0,0,17.92,11.62Z"/></svg></button>
					
                    </div>
						</div>
				</div>
			</section>